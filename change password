
import java.util.Scanner;

public class ChangePassword {

    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.println("Enter current password:");

        if ("examplePassword123".equals(scanner.nextLine())) System.out.println("Enter new password: " + (scanner.nextLine()));

        else System.out.println("Incorrect current password. Password change failed.");

        scanner.close();

    }

}
